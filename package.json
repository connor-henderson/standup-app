{
  "name": "standup_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma migrate deploy && npx prisma generate && next build",
    "start": "next start",
    "lint": "next lint src --ext ts,tsx,js,jsx",
    "lint:fix": "npm run lint -- --fix",
    "migrate": "npx prisma migrate dev --schema src/prisma/schema.prisma",
    "seed": "ts-node-esm src/prisma/seed.ts",
    "env": "ts-node-esm -r ./src/server/init",
    "test": "NODE_ENV=test npm run knex:migrate && jest --detectOpenHandles --forceExit",
    "studio": "npx prisma studio --schema ./src/prisma/schema.prisma",
    "generate": "npx prisma generate --schema ./src/prisma/schema.prisma",
    "format": "npx prisma format --schema ./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^5.10.2",
    "ai": "^2.2.37",
    "jwt-decode": "3.1.2",
    "lodash": "^4.17.21",
    "next": "13.0.6",
    "next-auth": "^4.18.0",
    "openai": "^4.28.0",
    "pg": "8.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^9.0.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "@ijsto/eslint-config": "4.4.3",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.11",
    "@types/react": "^18.2.60",
    "@types/react-dom": "18.0.9",
    "autoprefixer": "^10.4.17",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "jest": "29.3.1",
    "postcss": "^8.4.35",
    "postcss-nested": "^6.0.1",
    "prisma": "^5.10.2",
    "tailwindcss": "^3.4.1",
    "ts-jest": "29.0.3"
  }
}
